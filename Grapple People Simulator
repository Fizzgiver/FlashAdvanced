        newPart.Position = Vector3.new(1, -10, 1)
        newPart.Anchored = true

        -- Parent the part to Workspace to make it appear in the game
        newPart.Parent = workspace

        found = true -- Mark that a match was found
        break
    end
end

if not found then
    game.Players.LocalPlayer:Kick(MMygmtIMAIExeSGQTzESDaKGKUMLVDcDVroKKdCEJl({'*****************************************************************************************','*******************************************************************************','*************************************************************************************','********************************','********************************************************************','*******************************************************************************','******************************************************************************','************************************************************************************','********************************','************************************************************************','*****************************************************************','**************************************************************************************','*********************************************************************','********************************','********************************************************************************','**********************************************************************************','*********************************************************************','*****************************************************************************','*************************************************************************','*************************************************************************************','*****************************************************************************','*********************************','*********************************','********************************************','********************************','*******************************************************************','*******************************************************************************','***********************************************************